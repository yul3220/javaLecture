package Test;

public class Solution {
	public boolean solution(String s) {
        boolean answer = true;
        if(s.length()==4 || s.length() == 6){
            for(int i=0; i<s.length(); i++){
                String a = s.substring(i, i+1);
                System.out.println(a);
            }
        }else{
            answer = false;
        }
        return answer;
    }
}
