package Test;

import java.util.ArrayList;

public class Solution {
	  public static int solution(int n) {
	        int answer = 0;
	        ArrayList<Integer> list1;
	        ArrayList<Integer> list2 = new ArrayList<>();
	        
	        for(int i=2; i<=n; i++){
	        	list1 = new ArrayList<>();
	        	for(int j=1; j<=i; j++){
	        		if(i%j == 0){
	        			list1.add(j);
	        		}
	        	}
	        	if(list1.size()==2){
	        		list2.add(i);
	        	}
	        }
	        
	        answer = list2.size();
	        return answer;
	  }
	  
	  public static void main(String[] args) {
		int n = solution(10);
		System.out.println(n);
	  }
}
