import java.util.*;

class Solution {
	 public static String solution(String s) {
	        String answer = "";
	        for(int i=0; i<s.length(); i++){
	        	char a = s.charAt(i);
	        	for(int j=i+1; j<s.length(); j++){
	        		char b = s.charAt(j);
	        		System.out.println(b);
	        	}
	        	
	        }
	        return answer;
	    }
    
    public static void main(String[] args) {
    	String result = solution("Zbcdefg");
    	System.out.println(result);
    }
}
    
