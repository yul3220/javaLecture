package Test;

public class Solution {
	 public static int[] solution(int[] arr, int divisor) {
	        int[] answer = {};
	        for(int i=0; i<arr.length; i++){
	        	if(arr[i] % divisor == 0){
	        		answer[i] = arr[i];
	        	}
	        }
	        
	        for(int i=0; i<answer.length; i++){
	        	int min = i;
	        	for(int j=i+1; j<answer.length; i++){
	        		if(answer[min]>answer[j]){
	        			min = j;
	        		}
	        	}
	        	int temp = answer[i];
	        	answer[i] =answer[min];
	        	answer[min]=temp;
	        }
	        
	        if(answer.length == 0){
	        	answer[0] = -1;
	        }
	        return answer;
	 }
	 
	 public static void main(String[] args) {
		int[] arr = {5,9,7,10};
		int divisor = 5;
		int[] result = solution(arr, divisor);
		System.out.println(result);
	}
}
